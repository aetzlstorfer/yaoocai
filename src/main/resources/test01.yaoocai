
builtin function kill(exitCode: integer)                 -> vmfunc(0)
builtin function printInteger(value: integer)            -> vmfunc(1)
builtin function printBoolean(value: boolean)            -> vmfunc(2)
builtin function printString(value: String)              -> vmfunc(3)

function test1(a:integer, b:integer):integer {
  return a*b + test2(a, b);
}

function test2(a:integer, b:integer):integer {
  return a+b;
}

function main() {
  var a:integer = 5;
  var b:integer = 6;
  var result:integer;

  if(a == b) {
    result = test1(a, b);
  } else {
    result = test2(a, b);
  }

  printInteger(result);
}