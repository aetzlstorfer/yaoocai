builtin function testInput(): integer       -> test_in
builtin function testOutput(value: integer) -> test_out
builtin function fail()                     -> test_fail

function add(a: integer, b: integer) : integer {
  return a + b;
}

function main() {
  var a:integer = 5;
  var b:integer = a;
  var c:integer;
  c = 7;
  var d:integer = add(add(1, 2), 3);

  testOutput(a);
  testOutput(b);
